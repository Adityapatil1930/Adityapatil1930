#include<stdio.h>
#include<conio.h>
void main()
{
int a[5],b[5],c,i;
clrscr();
printf("\n enter the element of a:");
i=0;
while(i<5)
{
scanf("%d",%a[i]);
i++;
}
printf("\n enter the element b:");
i=0;
while(i<5)
{
scanf("%d",&b[i]);
i++;
}
printf("addition\n");
i=0;
while(i<5)
{
[i]=a[i]+b[i];
printf("\n%d+%d",a[i],b[i],c);
i++;
}
getch();
}
